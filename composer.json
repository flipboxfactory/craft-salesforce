{
  "name": "flipboxfactory/craft-salesforce",
  "description": "Salesforce Integration for Craft CMS",
  "version": "2.0.0",
  "type": "craft-plugin",
  "license": "proprietary",
  "keywords": [
    "cms",
    "craftcms",
    "salesforce",
    "force.com",
    "yii2"
  ],
  "require": {
    "php": ">=8.0",
    "craftcms/cms": "~4.0",
    "flipboxdigital/salesforce": "dev-feature/php8",
    "flipboxfactory/craft-ember": "~3.0",
    "flipboxfactory/craft-integration": "~3.0",
    "flipboxfactory/craft-psr3": "~2.0"
  },
  "require-dev": {
    "codeception/codeception": "^5.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\salesforce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\salesforce\\tests\\": "tests/src/"
    }
  },
  "support": {
    "email": "support@flipboxfactory.com",
    "issues": "https://github.com/flipboxfactory/craft-salesforce/issues?state=open",
    "source": "https://github.com/flipboxfactory/craft-salesforce",
    "docs": "https://github.com/flipboxfactory/craft-salesforce",
    "rss": "https://github.com/flipboxfactory/craft-salesforce/commits/master.atom"
  },
  "extra": {
    "name": "Salesforce",
    "handle": "salesforce",
    "class": "flipbox\\craft\\salesforce\\Force",
    "developer": "Flipbox Digital",
    "developerUrl": "https://www.flipboxdigital.com",
    "changelogUrl": "https://raw.githubusercontent.com/flipboxfactory/craft-salesforce/master/CHANGELOG.md",
    "downloadUrl": "https://github.com/flipboxfactory/craft-salesforce/archive/master.zip",
    "documentationUrl": "https://salesforce.flipboxfactory.com",
    "hasCpSection": true,
    "hasCpSettings": true,
    "schemaVersion": "1.2.0",
    "branch-alias": {
      "dev-master": "1.1.x-dev",
      "dev-develop": "1.2.x-dev"
    }
  }
}
